<h1>Add a picture</h1>
<%= form @pictures do |f|%>
  <p>
    <%= f.label "artist" %><br>
    <%= f.text_field "artist" %>
  </p>
  <p>
    <%= f.label "title" %><br>
    <%= f.text_field "title" %><br>
  </p>
    <p>
    <%= f.label "url" %><br>
    <%= f.text_field "url" %><br>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>